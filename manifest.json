{
    "name": "Ehentai helper",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to help download EHentai torrent file.",
    "icons": {
        "128": "icons/eh128x128.png"
    },
    "action":{
        "default_popup":"popup/popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
              "https://e-hentai.org/*"
            ],
            "js": ["content-scripts/content.js"]
        }
    ]
}